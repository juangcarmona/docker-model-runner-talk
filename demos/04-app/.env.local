# Local development environment variables
# These are used when running the app outside of Docker

# LLM API Configuration (point to localhost or running Docker model)
LLAMA_URL=http://localhost:11434/v1
LLAMA_MODEL=llama3.2:1B-Q8_0

# Application Configuration
PORT=8081
LOG_LEVEL=DEBUG
DEBUG=true

# Python specific
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1
